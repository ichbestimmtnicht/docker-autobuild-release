. ./hooks/env.sh

# Cycle through the arches

for SR_DEST_BUILD_ARCH in ${DEST_ARCHES}
do
    sh "${SR_DEST_BUILD_ARCH}.build.sh"
    rm -f "${SR_DEST_BUILD_ARCH}.build.sh"
done

exit 0
